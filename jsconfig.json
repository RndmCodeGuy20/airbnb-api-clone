{
	"compilerOptions": {
		"baseUrl": ".",
		"module": "ES6",
		"allowUnusedLabels": false,
		"moduleResolution": "node",
		"paths": {
			"#configs/*": ["src/config/*"],
			"#constants/*": ["./src/constants/*"],
			"#helpers/*": ["./src/helpers/*"],
			"#middlewares/*": ["./src/middlewares/*"],
			"#utils/*": ["./src/utils/*"],
		},
	},
	"exclude": ["node_modules"],
}
