{
	"name": "node-api-template",
	"version": "1.0.0",
	"description": "api boilerplate for robust and scalable REST type arch",
	"main": "src/index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=production npx babel-node src/index.js",
		"dev": "cross-env NODE_ENV=development nodemon --exec npx babel-node src/index.js",
		"test": "jest",
		"prepare": "husky",
		"commitlint": "commitlint --edit",
		"commit": "cz",
		"lint": "eslint src/**/*.js",
		"lint:fix": "eslint src/**/*.js --fix",
		"workflow:env": "npx babel-node ./.workflows/copy-env-keys.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RndmCodeGuy20/node-api-template.git"
	},
	"keywords": [
		"api",
		"boilerplate",
		"template",
		"REST"
	],
	"author": "RndmCodeGuy20",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/RndmCodeGuy20/node-api-template/issues"
	},
	"homepage": "https://github.com/RndmCodeGuy20/node-api-template#readme",
	"devDependencies": {
		"@babel/cli": "^7.23.9",
		"@babel/core": "^7.23.9",
		"@babel/node": "^7.23.9",
		"@babel/preset-env": "^7.23.9",
		"@commitlint/cli": "^18.6.0",
		"@commitlint/config-conventional": "^18.6.0",
		"commitizen": "^4.3.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.56.0",
		"eslint-config-google": "^0.14.0",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.1",
		"prettier": "^3.2.4"
	},
	"dependencies": {
		"@prisma/client": "^5.10.2",
		"bcrypt": "^5.1.1",
		"body-parser": "^1.20.2",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.4.1",
		"express": "^4.18.2",
		"express-winston": "^4.2.0",
		"helmet": "^7.1.0",
		"http-errors": "^2.0.0",
		"http-status-codes": "^2.3.0",
		"joi": "^17.12.1",
		"jsonwebtoken": "^9.0.2",
		"kafkajs": "^2.2.4",
		"morgan": "^1.10.0",
		"mysql2": "^3.9.2",
		"pg": "^8.11.3",
		"prisma": "^5.10.2",
		"redis": "^4.6.13",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.0",
		"timekeeper": "^2.3.1",
		"util": "^0.12.5",
		"uuid": "^9.0.1",
		"winston": "^3.11.0"
	},
	"imports": {
		"#configs/*": "./src/config/*.js",
		"#constants/*": "./src/constants/*.js",
		"#helpers/*": "./src/helpers/*.js",
		"#middlewares/*": "./src/middlewares/*.js",
		"#utils/*": "./src/utils/*.js"
	}
}
